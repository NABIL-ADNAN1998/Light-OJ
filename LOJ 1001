#include <bits/stdc++.h>
using namespace std;
using ll = long long;
int main()
{
    ll t;
    cin >> t;
    while (t--)
    {
        ll a;
        cin >> a;
        if (a % 2 == 0)
        {
            cout << a / 2 << " ";
            cout << a / 2 << endl;
            ;
        }
        else
        {
            cout << a / 2 + 1 << " ";
            cout << a - (a / 2 + 1) << endl;
        }
    }
    return 0;
}
